{
  "name": "carpenter-bar",
  "description": "A website for Carpenter Bar in downtown Sioux Falls, SD",
  "version": "1.0.0",
  "author": "Austin Mielke <amielkedev@gmail.com>",
  "dependencies": {
    "gatsby": "^2.0.19",
    "gatsby-image": "^2.0.14",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-netlify-cms": "^3.0.3",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-purgecss": "^2.4.0",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.6",
    "gatsby-source-filesystem": "^2.0.3",
    "gatsby-transformer-sharp": "^2.1.3",
    "netlify-cms": "^2.1.3",
    "react": "^16.5.1",
    "react-anchor-link-smooth-scroll": "^1.0.11",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/index.css",
