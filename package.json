    "build": "yarn run build:css && gatsby build",
    "develop": "yarn run build:css && gatsby develop",
    "format": "prettier --write '**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "postcss-cli": "^6.0.0",
    "prettier": "^1.14.2",
    "tailwindcss": "^0.6.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
